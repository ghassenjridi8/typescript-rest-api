{
  "name": "rest-api",
  "version": "1.0.0",
  "description": "REST API project with Express and TypeORM",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start-server": "node dist/server.js",
    "start-dev-server": "tsc-watch --onSuccess \"node dist/server.js 9002\"",
    "dev": "npm-run-all clean build start-dev-server",
    "populate-db": "npm-run-all clean build run-populate-db-script",
    "run-populate-db-script": "node dist/models/populate-db.js",
    "delete-db": "npm-run-all clean build run-delete-db-script",
    "run-delete-db-script": "node dist/models/delete-db.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "npm-run-all": "^4.1.5",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.2",
    "typeorm": "^0.3.20",
    "winston": "^3.19.0"
  }
}
